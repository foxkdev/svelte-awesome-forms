    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3",
    "typescript-tslint-plugin": "^1.0.2"
  }
